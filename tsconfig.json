{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "strictPropertyInitialization": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@pc/util": ["libs/util/src/index.ts"],
      "@pc/util/types": ["libs/util/src/types/index.ts"],
      "@pc/util/state": ["libs/util/src/state/index.ts"],
      "@pc/util/rxjs": ["libs/util/src/rxjs/index.ts"],
      "@pc/util/misc": ["libs/util/src/misc/index.ts"],
      "@pc/util/constants": ["libs/util/src/constants/index.ts"],
      "@pc/util/array": ["libs/util/src/array/index.ts"],
      "@pc/util/event-bus": ["libs/util/src/event-bus/index.ts"],
      "@pc/avanza": ["libs/avanza/src/index.ts"],
      "@pc/auto-trader/entity": ["libs/auto-trader/entity/src/index.ts"],
      "@pc/auto-trader/data-types": [
        "libs/auto-trader/data-types/src/index.ts"
      ],
      "@pc/nest/logger": ["libs/nest/logger/src/index.ts"],
      "@pc/nest/entity-service": ["libs/nest/entity-service/src/index.ts"],
      "@pc/auto-trader/backtest": ["libs/auto-trader/backtest/src/index.ts"],
      "@pc/angular/logger": ["libs/angular/logger/src/index.ts"],
      "@pc/angular/crud-api": ["libs/angular/crud-api/src/index.ts"],
      "@pc/angular/menu": ["libs/angular/menu/src/index.ts"],
      "@pc/angular/material": ["libs/angular/material/src/index.ts"],
      "@pc/angular/util": ["libs/angular/util/src/index.ts"],
      "@pc/angular/socket": ["libs/angular/socket/src/index.ts"],
      "@pc/nest/socket": ["libs/nest/socket/src/index.ts"],
      "@pc/angular/auth": ["libs/angular/auth/src/index.ts"],
      "@pc/nest/auth": ["libs/nest/auth/src/index.ts"],
      "@pc/auto-trader/shared": ["libs/auto-trader/shared/src/index.ts"],
      "@pc/power-comp/shared": ["libs/power-comp/shared/src/index.ts"],
      "@pc/power-comp/entity": ["libs/power-comp/entity/src/index.ts"],
      "@pc/angular/shared": ["libs/angular/shared/src/index.ts"],
      "@pc/nest/util": ["libs/nest/util/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
